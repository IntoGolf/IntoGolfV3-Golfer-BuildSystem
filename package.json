{
  "name": "intogolf-app-golfer",
  "version": "0.1.35",
  "description": "IntoGolfV3 Golfer app",
  "productName": "IntoGolfV3 Golfer app",
  "author": "IntoGolf <info@intogolf.nl>",
  "private": true,
  "scripts": {
    "start": "cross-env quasar dev",
    "build:web": "ENVIRONMENT=staging quasar build",
    "build:android": "cross-env ENVIRONMENT=staging quasar build -m android && npm run sign && open dist/cordova/android/apk/release",
    "build:staging:ios": "cross-env ENVIRONMENT=staging quasar build -m ios",
    "sign": "jarsigner -storepass 123456 -verbose -sigalg SHA1withRSA -digestalg SHA1 -keystore my-release-key.keystore dist/cordova/android/apk/release/app-release-unsigned.apk radar",
    "lint": "eslint --ext .js,.vue ./",
    "format": "prettier --write \"**/*.{js,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "test": "echo \"No test specified\" && exit 0"
  },
  "dependencies": {
    "@capacitor/camera": "^4.1.2",
    "@ionic/pwa-elements": "^3.1.1",
    "@quasar/extras": "^1.13.5",
    "axios": "^0.21.1",
    "cordova-plugin-whitelist": "^1.3.5",
    "core-js": "^3.22.2",
    "cross-env": "^7.0.3",
    "dayjs": "^1.11.1",
    "dotenv": "^16.0.0",
    "lodash": "^4.17.21",
    "macos-release": "^3.0.1",
    "next-vue-storage-watcher": "^0.0.7",
    "quasar": "^2.6.6",
    "vue": "^3.2.33",
    "vue-i18n": "^9.2.0-beta.34",
    "vue-qr": "^4.0.9",
    "vue-recaptcha-v3": "^2.0.1",
    "vue-router": "^4.0.0",
    "vue3-qrcode-reader": "^0.0.1",
    "vue3-storage": "^0.1.11",
    "yarn": "^1.22.18"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.13.14",
    "@quasar/app": "^3.0.0",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-vue": "^7.0.0",
    "eslint-webpack-plugin": "^2.4.0",
    "prettier": "^2.6.2"
  },
  "browserslist": [
    "last 10 Chrome versions",
    "last 10 Firefox versions",
    "last 4 Edge versions",
    "last 7 Safari versions",
    "last 8 Android versions",
    "last 8 ChromeAndroid versions",
    "last 8 FirefoxAndroid versions",
    "last 10 iOS versions",
    "last 5 Opera versions"
  ],
  "engines": {
    "node": ">= 12.22.1",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
