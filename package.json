{
  "name": "intogolf-app-golfer",
  "version": "0.1.41",
  "description": "IntoGolfV3 Golfer app",
  "productName": "IntoGolfV3 Golfer app",
  "author": "IntoGolf <info@intogolf.nl>",
  "private": true,
  "scripts": {
    "start": "cross-env quasar dev",
    "start:demo": "cross-env ENVIRONMENT=demo quasar dev",
    "start:dev": "cross-env ENVIRONMENT=dev quasar dev",
    "start:wilnis": "cross-env ENVIRONMENT=wilnis quasar dev",
    "start:development": "cross-env ENVIRONMENT=development quasar dev",
    "start:delfland": "cross-env ENVIRONMENT=delfland quasar dev",
    "start:engelenburg": "cross-env ENVIRONMENT=engelenburg quasar dev",
    "build:android": "cross-env ENVIRONMENT=staging quasar build -m android && npm run sign && open dist/cordova/android/apk/release",
    "build:batouwe:ios": "ENVIRONMENT=batouwe quasar build -m ios",
    "build:batouwe:android": "cross-env ENVIRONMENT=batouwe quasar build -m android",
    "build:delfland:web": "ENVIRONMENT=delfland quasar build",
    "build:delfland:ios": "ENVIRONMENT=delfland quasar build -m ios",
    "build:engelenburg:web": "ENVIRONMENT=engelenburg quasar build",
    "build:engelenburg:ios": "ENVIRONMENT=engelenburg quasar build -m ios",
    "build:engelenburg:android": "ENVIRONMENT=engelenburg quasar build -m android && npm run sign_eng",
    "build:demo:android": "cross-env ENVIRONMENT=demo quasar build -m android && npm run sign && open dist/cordova/android/apk/release",
    "build:demo:ios": "cross-env ENVIRONMENT=demo quasar build -m ios",
    "build:dev:ios": "ENVIRONMENT=dev quasar build -m ios",
    "build:development:web": "ENVIRONMENT=development quasar build",
    "build:development:ios": "ENVIRONMENT=development quasar build -m ios",
    "build:developmentandroid": "ENVIRONMENT=development quasar build -m android --warning-mode all && npm run sign && open dist/cordova/android/apk/release",
    "start:graafschap": "cross-env ENVIRONMENT=graafschap quasar dev",
    "build:graafschap:web": "ENVIRONMENT=graafschap quasar build",
    "build:graafschap:android": "cross-env ENVIRONMENT=graafschap quasar build -m android",
    "build:graafschap:ios": "ENVIRONMENT=graafschap quasar build -m ios",
    "build:kroonprins:ios": "ENVIRONMENT=kroonprins quasar build -m ios",
    "build:kroonprins:android": "ENVIRONMENT=kroonprins quasar build -m android",
    "start:maasduinen": "cross-env ENVIRONMENT=maasduinen quasar dev",
    "build:maasduinen:web": "ENVIRONMENT=maasduinen quasar build",
    "build:maasduinen:android": "cross-env ENVIRONMENT=maasduinen quasar build -m android",
    "build:maasduinen:ios": "ENVIRONMENT=maasduinen quasar build -m ios",
    "start:noordwijkse": "cross-env ENVIRONMENT=noordwijkse quasar dev",
    "build:noordwijkse:web": "ENVIRONMENT=noordwijkse quasar build",
    "build:noordwijkse:android": "cross-env ENVIRONMENT=noordwijkse quasar build -m android",
    "build:noordwijkse:ios": "ENVIRONMENT=noordwijkse quasar build -m ios",
    "build:sallandsche:web": "ENVIRONMENT=sallandsche quasar build",
    "build:sallandsche:android": "cross-env ENVIRONMENT=sallandsche quasar build -m android",
    "build:sallandsche:ios": "ENVIRONMENT=sallandsche quasar build -m ios",
    "build:web": "ENVIRONMENT=demo quasar build",
    "build:web_live": "quasar build",
    "build:wilnis:ios": "ENVIRONMENT=wilnis quasar build -m ios",
    "build:wilnis:android": "ENVIRONMENT=wilnis quasar build -m android",
    "sign": "jarsigner -storepass 123456 -verbose -sigalg SHA1withRSA -digestalg SHA1 -keystore my-release-key.keystore dist/cordova/android/apk/release/app-release-unsigned.apk radar",
    "sign_eng": "jarsigner -storepass mMdiPYRpw7iwbcctgRxA -verbose -sigalg SHA1withRSA -digestalg SHA1 -keystore keyEngelenBurg dist/cordova/android/apk/release/app-release-unsigned.apk radar",
    "lint": "eslint --ext .js,.vue ./",
    "format": "prettier --write \"**/*.{js,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "test": "echo \"No test specified\" && exit 0"
  },
